{
  "name": "cross-traffic-web-calculator",
  "description": "The simple web calculator of Highway Capacity Manual for the transportation engineers.",
  "version": "1.0.0",
  "author": "Rei Tamaru",
  "repository": {
    "type": "git",
    "url": "https://github.com/crosstraffic/cross-traffic-web-calculator"
  },
  "bugs": {
    "url": "https://github.com/crosstraffic/cross-traffic-web-calculator/issues"
  },
  "keywords": [
    "HCM",
    "Highway Capacity Manual",
    "Transportation Engineering",
    "Traffic Engineering",
    "Web Calculator"
  ],
  "scripts": {
    "modulize-wasm": "node HCM-middleware/modulize.js",
    "wasm": "wasm-pack build HCM-middleware --target web && npm run modulize-wasm",
    "dev": "npm run wasm && vite dev",
    "pre-build": "npm run wasm && vite build",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "3.3.1",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/google.accounts": "^0.0.15",
    "@types/jsonwebtoken": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@wasm-tool/rollup-plugin-rust": "^2.4.4",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.12",
    "daisyui": "^2.20.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "google-auth-library": "^9.15.0",
    "jsonwebtoken": "^9.0.2",
    "postcss": "^8.4.14",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "svelte": "^5.6.2",
    "svelte-check": "^4.0.9",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.1.7",
    "tslib": "^2.3.1",
    "types": "^0.1.1",
    "typescript": "^5.4.3",
    "vite": "^6.0.0",
    "vite-plugin-wasm-pack": "^0.1.12",
    "wasm-pack": "^0.13.1"
  },
  "type": "module",
  "dependencies": {
    "@supabase/ssr": "^0.4.1",
    "@supabase/supabase-js": "^2.47.2"
  }
}
