{
  "name": "cross-traffic-web-calculator",
  "version": "1.0.0",
  "scripts": {
    "modulize-wasm": "node ./HCM-middleware/modulize.js",
    "wasm": "wasm-pack build ./HCM-middleware --target web && npm run modulize-wasm",
    "dev": "npm run wasm && vite dev",
    "build": "pwd && which rustc && which cargo && which wasm-pack && npm run wasm && vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@playwright/test": "^1.22.0",
    "@sveltejs/adapter-auto": "3.1.1",
    "@sveltejs/kit": "^2.5.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.12",
    "daisyui": "^2.20.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.1.7",
    "tslib": "^2.3.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vite-plugin-wasm-pack": "^0.1.12",
    "wasm-pack": "^0.12.1"
  },
  "type": "module",
  "dependencies": {
    "@wasm-tool/rollup-plugin-rust": "^2.4.4"
  }
}
